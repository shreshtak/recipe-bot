{
    "version": 2,
    "builds": [
      {
        "src": "client-react/package.json",
        "use": "@vercel/static-build",
        "config": {
          "root": "client"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ],
    "ignore": [
        "server-python"
      ]
  }
  