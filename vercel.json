{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json", 
      "use": "@vercel/static-build",
      "config": {
        "root": "client" 
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/client/dist/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"  
    }
  ]
}
